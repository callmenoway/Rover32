# Database settings
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/rover32

# NextAuth configuration
NEXTAUTH_URL=http://localhost:3000
NEXTAUTH_SECRET=YourNextAuthSecretHere

# OAuth Providers
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret

GITHUB_ID=your_github_client_id
GITHUB_SECRET=your_github_client_secret

DISCORD_CLIENT_ID=your_discord_client_id
DISCORD_CLIENT_SECRET=your_discord_client_secret

# Cloudflare Turnstile for CAPTCHA protection
CLOUDFLARE_TURNSTILE_SECRET_KEY=your_turnstile_secret_key

# App settings
NODE_ENV=development
